{
  "name": "@prefabs.tech/vue3-user",
  "version": "0.30.0",
  "description": "Vue3 User Plugin",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/PrefabsTechVue3User.es.js",
      "require": "./dist/PrefabsTechVue3User.umd.js"
    },
    "./dist/PrefabsTechVue3User.css": "./dist/vue3-user.css"
  },
  "main": "./dist/PrefabsTechVue3User.umd.js",
  "module": "./dist/PrefabsTechVue3User.es.js",
  "types": "./dist/src/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "lint": "eslint . --ext .vue",
    "lint:fix": "eslint . --ext .vue --fix",
    "snapshot:update": "vitest --environment jsdom run --update",
    "sort-package": "npx sort-package-json",
    "test": "pnpm build && vitest --environment jsdom run --coverage --passWithNoTests",
    "test:component": "vitest --environment jsdom run component/ --passWithNoTests",
    "test:snapshot": "vitest --environment jsdom run snapshot/ --passWithNoTests",
    "test:unit": "vitest --environment jsdom run unit/ --passWithNoTests",
    "typecheck": "vue-tsc --noEmit -p tsconfig.json --composite false"
  },
  "dependencies": {
    "supertokens-web-js": "0.5.0",
    "supertokens-website": "16.0.1"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@prefabs.tech/eslint-config": "0.3.0",
    "@prefabs.tech/tsconfig": "0.2.1",
    "@prefabs.tech/vue3-config": "0.30.0",
    "@prefabs.tech/vue3-form": "0.30.0",
    "@prefabs.tech/vue3-i18n": "0.30.0",
    "@prefabs.tech/vue3-layout": "0.30.0",
    "@prefabs.tech/vue3-tanstack-table": "0.30.0",
    "@prefabs.tech/vue3-ui": "0.30.0",
    "@iconify/vue": "4.3.0",
    "@types/node": "24.10.1",
    "@vee-validate/i18n": "4.15.1",
    "@vee-validate/rules": "4.15.1",
    "@vee-validate/zod": "4.15.1",
    "@vitejs/plugin-vue": "6.0.4",
    "@vitejs/plugin-vue-jsx": "5.1.4",
    "@vitest/coverage-istanbul": "4.0.18",
    "@vue/test-utils": "2.4.6",
    "@vueuse/core": "12.7.0",
    "axios": "1.13.4",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-unicorn": "62.0.0",
    "eslint-plugin-vue": "10.7.0",
    "prettier": "3.8.1",
    "mitt": "3.0.1",
    "pinia": "3.0.4",
    "typescript": "5.5.4",
    "typescript-eslint": "8.54.0",
    "vee-validate": "4.15.1",
    "vite": "7.3.1",
    "vitest": "4.0.18",
    "vue": "3.5.27",
    "vue-eslint-parser": "10.2.0",
    "vue-router": "4.5.0",
    "vue-tsc": "1.2.0",
    "zod": "3.24.2"
  },
  "peerDependencies": {
    "@prefabs.tech/vue3-config": "0.30.0",
    "@prefabs.tech/vue3-i18n": "0.30.0",
    "@prefabs.tech/vue3-layout": "0.30.0",
    "@prefabs.tech/vue3-ui": "0.30.0",
    "@vee-validate/i18n": ">=4.7.1",
    "@vee-validate/rules": ">=4.7.1",
    "@vueuse/core": ">=9.4.0",
    "axios": ">=0.26.1",
    "pinia": ">=2.0",
    "vee-validate": ">=4.7.1",
    "vue": ">=3.2",
    "vue-router": ">=4.0",
    "zod": ">=3.20.6"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  }
}
