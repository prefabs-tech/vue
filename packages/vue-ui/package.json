{
  "name": "@prefabs.tech/vue3-ui",
  "version": "0.29.1",
  "description": "Vue3 UI Component Library",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/PrefabsTechVue3UI.es.js",
      "require": "./dist/PrefabsTechVue3UI.umd.js"
    },
    "./dist/PrefabsTechVue3UI.css": "./dist/vue3-ui.css"
  },
  "main": "./dist/PrefabsTechVue3UI.umd.js",
  "module": "./dist/PrefabsTechVue3UI.es.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "lint": "eslint . --ext .vue",
    "lint:fix": "eslint . --ext .vue --fix",
    "snapshot:update": "vitest --environment jsdom run --update",
    "sort-package": "npx sort-package-json",
    "test": "vitest --environment jsdom run --coverage --passWithNoTests",
    "test:component": "vitest --environment jsdom run component/ --passWithNoTests",
    "test:snapshot": "vitest --environment jsdom run snapshot/ --passWithNoTests",
    "test:unit": "vitest --environment jsdom run unit/ --passWithNoTests",
    "typecheck": "vue-tsc --noEmit -p tsconfig.json --composite false"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@prefabs.tech/eslint-config": "0.2.1",
    "@prefabs.tech/tsconfig": "0.2.1",
    "@types/node": "24.10.1",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitejs/plugin-vue-jsx": "4.1.1",
    "@vitest/coverage-istanbul": "4.0.18",
    "@vue/test-utils": "2.4.6",
    "@vueuse/core": "12.7.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-unicorn": "60.0.0",
    "eslint-plugin-vue": "10.7.0",
    "prettier": "3.8.1",
    "typescript": "5.5.4",
    "vite": "6.1.1",
    "vitest": "4.0.18",
    "vue": "3.5.27",
    "vue-draggable-next": "2.2.1",
    "vue-eslint-parser": "10.2.0",
    "vue-tsc": "1.2.0"
  },
  "peerDependencies": {
    "vue": ">=3.2"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  }
}
