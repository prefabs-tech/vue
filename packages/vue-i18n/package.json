{
  "name": "@prefabs.tech/vue3-i18n",
  "version": "0.29.1",
  "description": "Vue3 I18n Plugin",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/PrefabsTechVue3I18n.es.js",
      "require": "./dist/PrefabsTechVue3I18n.umd.js"
    },
    "./dist/PrefabsTechVue3I18n.css": "./dist/vue3-i18n.css"
  },
  "main": "./dist/PrefabsTechVue3I18n.umd.js",
  "module": "./dist/PrefabsTechVue3I18n.es.js",
  "types": "./dist/src/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "lint": "eslint . --ext .vue",
    "lint:fix": "eslint . --ext .vue --fix",
    "snapshot:update": "vitest --environment jsdom run --update",
    "sort-package": "npx sort-package-json",
    "test": "vitest --environment jsdom run --coverage --passWithNoTests",
    "test:component": "vitest --environment jsdom run component/ --passWithNoTests",
    "test:snapshot": "vitest --environment jsdom run snapshot/ --passWithNoTests",
    "test:unit": "vitest --environment jsdom run unit/ --passWithNoTests",
    "test:watch": "vitest",
    "typecheck": "vue-tsc --noEmit -p tsconfig.json --composite false"
  },
  "dependencies": {
    "vue-i18n": "9.1.10"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@prefabs.tech/eslint-config": "0.2.1",
    "@prefabs.tech/tsconfig": "0.2.1",
    "@prefabs.tech/vue3-config": "0.29.1",
    "@types/node": "24.10.1",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitejs/plugin-vue-jsx": "4.1.1",
    "@vitest/coverage-istanbul": "4.0.18",
    "@vueuse/core": "12.7.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-unicorn": "62.0.0",
    "eslint-plugin-vue": "10.7.0",
    "jsdom": "27.0.1",
    "pinia": "3.0.4",
    "prettier": "3.8.1",
    "typescript": "5.5.4",
    "vite": "6.1.1",
    "vitest": "4.0.18",
    "vue": "3.5.27",
    "vue-eslint-parser": "10.2.0",
    "vue-tsc": "1.2.0"
  },
  "peerDependencies": {
    "@prefabs.tech/vue3-config": "0.29.1",
    "@vueuse/core": ">=9.4.0",
    "pinia": ">=2.0",
    "vue": ">=3.2"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  }
}
